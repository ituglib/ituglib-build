2test -z "${VERSION_PATH}" && \
	export VERSION=`${WORKSPACE}/src/wget --version | \
		grep '^GNU Wget2 ' | \
		sed '1,$s/GNU Wget2 \([^ ]*\) .*/\1/'` \
 || \
	export VERSION=`${VERSION_PATH} --version | \
		grep '^GNU Wget2 ' | \
		sed '1,$s/GNU Wget2 \([^ ]*\) .*/\1/'`
echo "Packaging wget2 version ${VERSION}"

