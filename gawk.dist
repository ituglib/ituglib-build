test -z "${VERSION_PATH}" && \
	export VERSION=`${WORKSPACE}/git --version | fgrep 'gawk version' | \
		sed '1,$s/gawk --version \(.*\)\.[^.]*$/\1/'` \
 || \
	export VERSION=`${VERSION_PATH} --version | fgrep 'gawk --version' | \
		sed '1,$s/gawk --version \(.*\)\.[^.]*$/\1/'`
echo "Packaging gawk version ${VERSION}"

